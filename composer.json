{
    "name": "akeneo/phpspec-skip-example-extension",
    "description": "Skip your PhpSpec examples through annotations",
    "license": "MIT",
    "authors": [
        {
            "name": "Gildas Quéméner",
            "email": "gildas@akeneo.com"
        }
    ],
    "require": {
        "phpspec/phpspec": "2.1.*"
    },
    "autoload": {
        "psr-0": {
            "Akeneo": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev",
            "dev-1.0": "1.0.x-dev"
        }
    }
}
